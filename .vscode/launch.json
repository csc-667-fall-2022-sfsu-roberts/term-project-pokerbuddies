    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach by Process ID",
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "port": 3000
        },
        
        {
            "name": "Launch Program",
            "program": "${file}",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        }
        
        
    ]
}